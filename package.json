{
    "name": "server",
    "version": "1.0.0",
    "description": "Backend for my MERN portfolio site.",
    "main": "server/server.js",
    "type": "module",
    "author": {
        "name": "Sean G Brown",
        "url": "https://seabAu.github.com/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seabAu/seangb-portfolio.git"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/seabAu/seangb-portfolio/issues"
    },
    "scripts": {
        "server": "nodemon server/server.js",
        "client": "npm dev --prefix client",
        "clientstart": "npm start --prefix client",
        "clientinstall": "npm install --prefix client",
        "clientbuild": "npm build --prefix client",
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "build": "npm install && npm run clientinstall && npm run clientbuild",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "concurrently \"npm run server\" \"npm run dev --prefix client\"",
        "startapp": "concurrently \"npm run server\" \"npm run clientstart\"",
        "buildapp": "concurrently \"npm run server\" \"npm run clientbuild\"",
        "heroku postbuild": "NPM_CONFIG_PRODUCTION=false npm install -prefix client && npm run build --prefix client",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "app-start-dev": "npm run client-start-dev --prefix client",
        "start-dev": "concurrently \"npm run server\" \"npm run app-start-dev\"",
        "app-build-dev": "npm run client-build-dev --prefix client",
        "build-dev": "concurrently \"npm run server\" \"npm run app-build-dev\"",
        "app-start-prod": "npm run client-start-prod --prefix client",
        "start-prod": "concurrently \"npm run server\" \"npm run app-start-prod\"",
        "app-build-prod": "npm run client-build-prod --prefix client",
        "build-prod": "concurrently \"npm run server\" \"npm run app-build-prod\""
    },
    "dependencies": {
        "async": "^3.2.4",
        "asynckit": "^0.4.0",
        "axios": "^1.7.2",
        "base64-js": "^1.5.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.1",
        "buffer-equal-constant-time": "^1.0.1",
        "color-convert": "^2.0.1",
        "color-name": "^1.1.4",
        "concurrently": "^7.6.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "env-cmd": "^10.1.0",
        "express": "^4.18.2",
        "express-jwt": "^8.4.1",
        "gh-pages": "^5.0.0",
        "http-errors": "^2.0.0",
        "http-proxy": "^1.18.1",
        "http-proxy-middleware": "^2.0.6",
        "immer": "^9.0.19",
        "jsonwebtoken": "^9.0.2",
        "jwa": "^2.0.0",
        "jws": "^4.0.0",
        "lodash": "^4.17.21",
        "mongodb": "^4.13.0",
        "mongoose": "^6.9.0",
        "mongoose-autopopulate": "^1.0.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemon": "^3.1.7",
        "path": "^0.12.7",
        "rxjs": "^7.8.0",
        "sharp": "^0.33.4",
        "socks": "^2.7.1"
    },
    "devDependencies": {}
}
